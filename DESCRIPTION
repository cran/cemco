Package: cemco
Title: Fit 'CemCO' Algorithm
Version: 0.2
Authors@R: 
   c(person('Carlos', 'Relvas', role = c('aut'), email = 'carlos.edu.relvas@gmail.com'), person('Andre', 'Fujita', role = c('aut','cre'), email = 'andrefujita@usp.br'))
